<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{ 
    // TODO #3 - Use the same list display from Views/Search/Index.cshtml 

     @foreach (var jobList in ViewBag.jobs)
    {
        <table class="job-listing">
            @foreach (KeyValuePair<string, string> job in jobList)
            {
                <tr>
                    <td>@job.Key</td>
                    <td>@job.Value</td>
                </tr>
            }
        </table>
    }
}